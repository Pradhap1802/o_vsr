<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Override stock.move tree view to change product_uom_qty field label -->
        <record id="stock_move_tree_custom" model="ir.ui.view">
            <field name="name">stock.move.tree.custom</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_uom_qty']" position="attributes">
                    <attribute name="string">Actual Received Quantity</attribute>
                </xpath>
            </field>
        </record>

        <!-- Override stock.picking move tree view to change product_uom_qty field label -->
        <record id="stock_picking_move_tree_custom" model="ir.ui.view">
            <field name="name">stock.picking.move.tree.custom</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_picking_move_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_uom_qty']" position="attributes">
                    <attribute name="string">Actual Received Quantity</attribute>
                </xpath>
            </field>
        </record>

        <!-- Override stock.move operations form view to change product_uom_qty field label -->
        <record id="stock_move_operations_form_custom" model="ir.ui.view">
            <field name="name">stock.move.operations.form.custom</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_stock_move_operations"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_uom_qty']" position="attributes">
                    <attribute name="string">Actual Received Quantity</attribute>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
