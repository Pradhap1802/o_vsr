<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_move_form_inherit_tally" model="ir.ui.view">
        <field name="name">account.move.form.inherit.tally</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Tally" name="tally">
                    <group>
                        <group>
                            <field name="x_studio_tally_sales_no"/>
                            <field name="x_studio_tally_purchase_no"/>
                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
